<div class="container login-container">
  <div class="row">
    <div class="col s5 push-s3">
    <div class="center">
      <h2>Log in</h2>
      
    </div>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :email, "Email (guest@biancasemprit.me)" %><br />
    <%= f.email_field :email, autofocus: true, :value => "guest@biancasemprit.me" %>
  </div>

  <div class="field">
    <%= f.label :password, "Password (guest123)" %><br />
    <%= f.password_field :password, autocomplete: "off", :value => "guest123" %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>

  <div class="actions">
    <%= f.submit "Log in", class: "login-button" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
    </div>
  </div>
</div>
